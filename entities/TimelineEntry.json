{
  "name": "TimelineEntry",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "description": "Title of the schedule item"
    },
    "description": {
      "type": "string",
      "description": "Detailed description of the event"
    },
    "date": {
      "type": "string",
      "format": "date",
      "description": "Date of the event"
    },
    "category": {
      "type": "string",
      "enum": [
        "milestone",
        "achievement",
        "event",
        "project",
        "personal"
      ],
      "default": "event",
      "description": "Category of the schedule item"
    },
    "importance": {
      "type": "string",
      "enum": [
        "low",
        "medium",
        "high",
        "critical"
      ],
      "default": "medium",
      "description": "Importance level of the entry"
    }
  },
  "required": [
    "title",
    "date"
  ]
}